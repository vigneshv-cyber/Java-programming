import java.util.*;
public class facto{
public static void main(String[] args){
Scanner obj=new Scanner(System.in);
try{
int n,fact=1;
System.out.println("enter the number for factorial:");
n=obj.nextInt();
if(n>0){
for(int i=1;i<=n;i++){
fact=fact*i;
}
System.out.print(fact);
}
else{
System.out.print("invalid:");
}
}
catch(InputMismatchException a){
System.out.print("invalid:");
}
}
}
