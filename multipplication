import java.util.Scanner;
public class table{
public static void main(String array[]){
Scanner obj=new Scanner(System.in);
int n;
System.out.println("which number table you want?");
int a=obj.nextInt();
System.out.println("bottom number:");
int m=obj.nextInt();
if(m>0 && a>0){
for(int i=1;i<=m;i++){
int b=a*i;
System.out.print(a);
System.out.print("*");
System.out.print(i);
System.out.print("=");
System.out.print(b);
System.out.println(" ");
}
}
else{
System.out.println("invalid");
}
}
}
